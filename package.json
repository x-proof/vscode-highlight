{
  "name": "x-proof",
  "displayName": "X-Proof",
  "description": "Syntax highlighting for X-Proof language.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.70.0"
  },
  "publisher": "Zecyel",
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "x-proof",
      "aliases": ["X-Proof", "x-proof", "xp"],
      "extensions": [".xp", ".xproof", ".xprf"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "x-proof",
      "scopeName": "source.xp",
      "path": "./syntaxes/x-proof.tmLanguage.json"
    }]
  }
}
